        </div>
        <!--End nav-tabs-->
        <div class="tab-content wow fadeIn animated" id="myTabContent">
          <div
            class="tab-pane fade show active"
            id="tab-one"
            role="tabpanel"
            aria-labelledby="tab-one"
          >
            <div class="row product-grid-4">
              <% homeList.forEach(function(homeList,index) { %>

              <div class="col-lg-3 col-md-4 col-12 col-sm-6">
                <div class="product-cart-wrap mb-30">
                  <div class="product-img-action-wrap d-flex justify-content-center">
                    <div class="product-img product-img-zoom" style="height: 180px;width: 236px;">
                      <a href="/product_details/<%= homeList._id%>">
                        <img
                          class="default-img"
                          src="/uploads/<%= homeList.Image[0] %>"
                          alt=""
                        />
                        <!-- <img class="hover-img" src="assets/imgs/shop/product-7-1.jpg" alt=""> -->
                      </a>
                    </div>
                    <div class="product-action-1">
                      <a
                        aria-label="Quick view"
                        class="action-btn small hover-up"
                        data-bs-toggle="modal"
                        data-bs-target="#quickViewModal
  "
                        ><i class="fi-rs-search"></i
                      ></a>
                      <a
                        aria-label="Add To Wishlist"
                        class="action-btn hover-up"
                        onclick="addtowishlist('<%= homeList._id%>');Swal.fire({
                        position: 'center',
                        icon: 'success',
                        title: 'Product is successfully added to Wishlist',
                        showConfirmButton: false,
                        timer: 1500
                      })"
                        ><i class="fi-rs-heart"></i
                      ></a>
                      <a
                        aria-label="Compare"
                        class="action-btn small hover-up"
                        href=""
                        tabindex="0"
                        ><i class="fi-rs-shuffle"></i
                      ></a>
                    </div>
                    <div
                      class="product-badges product-badges-position product-badges-mrg"
                    ></div>
                  </div>
                  <div class="product-content-wrap">
                    <div class="product-category">
                      <a href="shop-grid-right.html"><%=homeList.category%></a>
                    </div>
                    <h2>
                      <a href="shop-product-right.html"
                        ><%=homeList.Productname%></a
                      >
                    </h2>
                    <div class="rating-result" title="90%">
                      <span>
                        <span>95%</span>
                      </span>
                    </div>
                    <div class="product-price">
                      â‚¹<span><%=homeList.Price%></span>
                    </div>
                    <div class="product-action-1 show">
                      <button
                        type="button"
                        aria-label="Add To Cart"
                        class="action-btn hover-up"
                        onclick="addtocart('<%-homeList._id%>');Swal.fire({
                                  position: 'center',
                                  icon: 'success',
                                  title: 'Product is successfully added to cart',
                                  showConfirmButton: false,
                                  timer: 1500
                                })"
                      >
                        <i class="fi-rs-shopping-bag-add"></i>
                      </button>
                    </div>
                  </div>
                </div>
              </div>
              <% }) %>
            </div>
            <!--End product-grid-4-->
          </div>
          <!--En tab one (Featured)-->

          <!--En tab two (Popular)-->

          <!--En tab three (New added)-->
        </div>
        <!--End tab-content-->
      </div>
